<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dlu.cdmanage.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dlu.cdmanage.entity.Orders">
        <id column="id" property="id" />
        <result column="ref" property="ref" />
        <result column="cdNum" property="cdNum" />
        <result column="cdId" property="cdId" />
        <result column="price" property="price" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ref, cdNum, cdId, price
    </sql>

</mapper>
